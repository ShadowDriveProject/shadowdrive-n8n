# N8N Base Settings
N8N_HOST=n8n.onrender.com
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_EDITOR_BASE_URL=https://n8n.onrender.com
WEBHOOK_TUNNEL_URL=https://n8n.onrender.com

# Auth (poți schimba user/pass)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Executions + Database
DB_TYPE=postgresdb
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=n8n

# Others
EXECUTIONS_MODE=queue
QUEUE_MODE=redis
N8N_LOG_LEVEL=info

# Telegram (opțional - pentru notificări Telegram)
TELEGRAM_TOKEN=___INSEREAZĂ_TOKENUL_TĂU_AICI___
TELEGRAM_CHAT_ID=___INSEREAZĂ_CHAT_ID_AICI___

# Timezone & SSL
GENERIC_TIMEZONE=Europe/Amsterdam
